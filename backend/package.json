{
  "name": "sichrplace-backend",
  "version": "1.0.0",
  "description": "Backend API for SichrPlace - a property rental and management platform.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:supabase": "echo 'Starting SichrPlace with Supabase...' && node server.js",
    "dev": "nodemon server.js",
    "dev:supabase": "echo 'Development mode with Supabase...' && nodemon server.js",
    "test:connection": "node -e \"require('./config/supabase').testConnection()\"",
    "test:db": "node -e \"const {supabase} = require('./config/supabase'); supabase.from('users').select('count').limit(1).then(r => console.log('DB OK:', !r.error)).catch(e => console.error('DB Error:', e.message))\"",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "test": "jest --testTimeout=10000",
    "test:watch": "jest --watch",
    "test:real": "jest --testNamePattern='Real Data' --testTimeout=60000 --runInBand",
    "test:step4-real": "jest tests/step4-real-data.test.js --testTimeout=60000",
    "test:api-real": "node tests/step4-real-api-tester.js",
    "test:schema": "node tests/complete-schema-test.js",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "test:google-maps": "jest tests/step9-2-google-maps-100-coverage.test.js",
    "test:paypal": "jest tests/paypal-integration-100.test.js",
    "test:step4": "jest --testPathPattern=step4",
    "test:api": "jest --testPathPattern=step4-api",
    "test:frontend": "jest --testPathPattern=step4-frontend",
    "test:integration": "jest --testNamePattern='Integration Tests'",
    "test:verbose": "jest --verbose",
    "test:silent": "jest --silent",
    "build": "echo 'Build process completed - SichrPlace ready for deployment'",
    "build:prod": "NODE_ENV=production npm run build",
    "build:staging": "NODE_ENV=staging npm run build",
    "audit:fix": "npm audit fix",
    "security:check": "npm audit --audit-level moderate",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "clean": "rm -rf coverage node_modules/.cache",
    "prepare": "npm run clean"
  },
  "keywords": [
    "express",
    "postgresql",
    "supabase",
    "jwt",
    "property",
    "rental",
    "backend",
    "api"
  ],
  "author": "omer3kale",
  "license": "MIT",
  "dependencies": {
    "@microsoft/clarity": "^1.0.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@supabase/realtime-js": "^2.14.0",
    "@supabase/supabase-js": "^2.53.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "lusca": "^1.7.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "redis": "^5.8.0",
    "stripe": "^18.4.0",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.15.15",
    "web-push": "^3.6.7",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "chai": "^5.2.1",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jest-sonar-reporter": "^2.0.0",
    "mocha": "^11.7.1",
    "nodemon": "^3.1.0",
    "nyc": "^17.1.0",
    "proxyquire": "^2.1.3",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omer3kale/SichrPlace.git"
  },
  "bugs": {
    "url": "https://github.com/omer3kale/SichrPlace/issues"
  },
  "homepage": "https://github.com/omer3kale/SichrPlace#readme"
}
