# SichrPlace Environment Variables Template
# Copy this file to .env.production.clean and fill in your actual values

# Production Environment
SUPABASE_PROJECT_REF_PROD=your-production-project-ref
SUPABASE_URL_PROD=https://your-production-project-ref.supabase.co
SUPABASE_ANON_KEY_PROD=your-production-anon-key

# Staging Environment  
SUPABASE_PROJECT_REF_STAGING=your-staging-project-ref
SUPABASE_URL_STAGING=https://your-staging-project-ref.supabase.co
SUPABASE_ANON_KEY_STAGING=your-staging-anon-key

# Supabase Access Token
SUPABASE_ACCESS_TOKEN=your-supabase-access-token

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Database Configuration
DATABASE_URL_PROD=postgresql://postgres:your-password@db.your-prod-ref.supabase.co:5432/postgres
DATABASE_URL_STAGING=postgresql://postgres:your-password@db.your-staging-ref.supabase.co:5432/postgres
