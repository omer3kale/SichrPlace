{
  "name": "sichrplace",
  "version": "1.0.0",
  "description": "Secure apartment rental platform connecting landlords and tenants",
  "main": "server.js",
  "scripts": {
    "start": "cd backend && node server.js",
    "dev": "cd backend && nodemon server.js",
    "backend": "cd backend && node server.js",
    "backend:dev": "cd backend && nodemon server.js",
    "test": "jest",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:backend": "jest --selectProjects backend-commonjs --runInBand",
    "test:netlify": "jest --selectProjects netlify-functions --runInBand",
    "test:frontend": "jest --selectProjects frontend-js --runInBand",
    "preflight": "node scripts/preflight.js",
    "build": "npm run preflight && echo Build checks passed",
    "ci:verify": "npm run lint && npm run preflight",
    "preflight:strict": "npm run preflight -- --strict",
    "secrets:scan": "node scripts/secret-scan.js",
    "lint": "eslint . --ext .js,.mjs",
    "lint:esm": "eslint netlify/functions scripts organized/js --ext .js,.mjs",
    "format": "prettier --write .",
    "security:audit": "npm audit --audit-level=info",
    "security:fix": "npm audit fix --force",
    "security:check": "npx audit-ci --critical --high --moderate",
    "security:update": "npm update && npm audit fix",
    "security:comprehensive": "node scripts/dependency-security-manager.mjs",
    "security:comprehensive:fix": "node scripts/dependency-security-manager.mjs --fix",
    "security:track": "node scripts/version-tracker.mjs",
    "security:track:fix": "node scripts/version-tracker.mjs --fix",
    "security:full": "npm run security:comprehensive && npm run security:track",
    "security:full:fix": "npm run security:comprehensive:fix && npm run security:track:fix",
    "seo:check": "bash scripts/seo-validation.sh",
    "seo:lighthouse": "lighthouse https://sichrplace.com --only-categories=seo --view",
    "deploy:validate": "npm run seo:check && npm run security:audit"
  },
  "keywords": [
    "apartment",
    "rental",
    "property",
    "landlord",
    "tenant",
    "real-estate"
  ],
  "author": "SichrPlace Team",
  "license": "MIT",
  "dependencies": {
    "@netlify/functions": "^4.2.1",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@paypal/paypal-server-sdk": "^1.1.0",
    "@supabase/supabase-js": "^2.39.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "buffer": "^6.0.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "events": "^3.3.0",
    "express": "^4.21.1",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "fs-extra": "^11.3.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "lusca": "^1.7.0",
    "mime-types": "^3.0.1",
    "morgan": "^1.10.1",
    "multer": "1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "querystring": "^0.2.1",
    "sharp": "^0.33.5",
    "stream": "^0.0.3",
    "stripe": "^18.5.0",
    "swagger-ui-express": "^5.0.1",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "uuid": "^11.0.3",
    "validator": "^13.11.0",
    "web-push": "^3.6.7",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.3",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^30.0.0",
    "babel-jest": "^29.7.0",
    "bcrypt": "^6.0.0",
    "chai": "^4.3.10",
